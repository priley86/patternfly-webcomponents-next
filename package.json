{
  "name": "patternfly-webcomponents-test",
  "version": "0.0.0-semantically-released",
  "description":
    "This project will provide a set of core web components for the [PatternFly](https://www.patternfly.org) project.",
  "main": "lib/index.js",
  "scripts": {
    "commit": "git-cz",
    "build": "webpack --colors",
    "test": "npm-run-all --serial prettier lint karma",
    "karma": "karma start karma.conf.js",
    "lint": "eslint --max-warnings 0 src",
    "prettier": "prettier --write --single-quote --no-semi 'src/**/*.js'",
    "build:all": "npm-run-all --serial build build:react build:angular",
    "build:react": "cd frameworks/react && npm install && npm run build",
    "build:angular": "cd frameworks/angular && npm install && npm run build",
    "test:all": "npm-run-all --serial test test:react test:angular",
    "test:react": "cd frameworks/react && npm run test",
    "test:angular": "cd frameworks/angular && npm run test",
    "prepublish": "npm run build",
    "semantic-release":
      "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patternfly/patternfly-webcomponents.git"
  },
  "author": "Red Hat",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/patternfly/patternfly-webcomponents/issues"
  },
  "homepage": "https://github.com/patternfly/patternfly-webcomponents#readme",
  "devDependencies": {
    "@webcomponents/custom-elements": "^1.0.0",
    "@webcomponents/webcomponentsjs": "^1.0.0",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "commitizen": "^2.9.6",
    "css-loader": "^0.28.7",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^2.1.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "jasmine-core": "^2.5.2",
    "jasmine-promises": "^0.4.1",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "npm-run-all": "^4.1.1",
    "patternfly": "^3.27.1",
    "prettier": "^1.3.1",
    "promise-polyfill": "^6.0.2",
    "semantic-release": "^6.3.6",
    "skatejs": "^4.6.7",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5"
  },
  "peerDependencies": {
    "patternfly": "^3.27.1",
    "skatejs": "^4.6.7",
    "@webcomponents/custom-elements": "^1.0.0",
    "@webcomponents/webcomponentsjs": "^1.0.0"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "directories": {
    "lib": "lib"
  }
}
